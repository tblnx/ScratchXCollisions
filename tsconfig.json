{
    "compilerOptions": {
        "target": "es6",
        "sourceMap": true,
        "allowJs": true,
        "checkJs": true,
        "watch": true,
        "noImplicitAny": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true
    },
    "include": [
        "src/**/*"
    ],
    "exclude": [
        "node_modules/**/*"
    ],
    "typeAcquisition": {
        "include": [
         "jquery", "node"
     ]
    }
}